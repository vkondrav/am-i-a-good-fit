{
  "manifest_version": 3,
  "name": "Am I a Good Fit?",
  "version": "1.0",
  "description": "Inline analysis of job description and your resume",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/jobs/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}